Proceso doblar_nuemero
	Escribir "Algoritmo Doblar Numero";
	Escribir"====================";
	Escribir "";//Salto de linea
	//Paso 1 Solicitar el numero entero
	Definir num Como Entero;
	Escribir "Ingresar el numero" ;
	Leer num;
	//Paso 2 Doblar el numero y multiplicar por 25
	Definir res Como Entero;
	res<-num*2*25;
	//Paso 3 Dar a conocer el resultado
	Escribir "El resultado es",res;
FinProceso
